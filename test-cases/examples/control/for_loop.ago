fun main(){
    var i = 0
    Trace.print('test 1')
    for(; i< 3; i++){
        Trace.print(i)     // 0 1  2
    }

    Trace.print('test 2')
    for(; i >= 0; ){
        Trace.print(i--)   // 3 2 1 0
    }

    Trace.print('test 3')
    for(;;){
        Trace.print(i++)   // -1 0 1 2 3 4 5
        if(i > 5) break;
    }

    Trace.print('test 4')
    var t = 0;
    var j as int
    var k as int
    for(j = 1, k = 2; j < 5; j+=2, k *= 2){
        t += j + k;     // 1 2; 3 4;
    }
    Trace.print(t);    // 10
}