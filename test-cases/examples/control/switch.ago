fun main(){
    var day = 4
    switch (day) {
        case 1:
            Trace.print("星期一");
            break;
        case 4:
            Trace.print("星期四");
        case 3:
            Trace.print("星期三");
            break;
        case 2:
            Trace.print("星期二");
            break;
        case 5:
            Trace.print("星期五");
            break;
        default:
            Trace.print("周末或无效输入");
    }
}