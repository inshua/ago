fun main() {
    fun test(){
        Trace.print("test");
    }
    fun add(a as int, b as int) as int{
        return a + b
    }

    var r = runspace.create();      // TODO runspace shouldn't submit tasks, if wan't preserve runspaces within same context, use `fork|await ... via ForkContext`

    r.runAsync(new test())
    sleep(100)

    var c = r.run(new add(1, 2));
    Trace.print(c)

}