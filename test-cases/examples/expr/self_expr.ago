class Staff{
    age as int = 22
    salary as int {get; set;} = 2000
    fav as int[]
    fun new(){
        fav = [int[]| 1, 2, 3];
    }
}

fun test1(){
    var i = 2;
    var j = i++
    Trace.print("i=" + i + " j=" + j)     // 3 2
    j = ++i
    Trace.print("i=" + i + " j=" + j)     // 4 4

    var staff = new Staff()
    j = staff.age ++
    Trace.print("age=" + staff.age + " j=" + j)     // 23  22
    j = ++ staff.age
    Trace.print("age=" + staff.age + " j=" + j)     // 24  24


    j = staff.salary ++
    Trace.print("salary=" + staff.salary + " j=" + j)     // 2001 2000
    var j = ++ staff.salary
    Trace.print("salary=" + staff.salary + " j=" + j)     // 2002  2002

    j = (staff.salary *= 2)
    Trace.print("salary=" + staff.salary + " j=" + j)     // 4004 4004

    j = staff.fav[1] ++
    Trace.print("fav[1]=" + staff.fav[1] + " j=" + j)     // 3 2
    var j = ++ staff.fav[1]
    Trace.print("fav[1]=" + staff.fav[1] + " j=" + j)     // 4  4
}

fun test2(){
    var i = 2;
    var j = i--
    Trace.print("i=" + i + " j=" + j)     // 1 2
    j = --i
    Trace.print("i=" + i + " j=" + j)     // 0 0

    var staff = new Staff()
    j = staff.age --
    Trace.print("age=" + staff.age + " j=" + j)     // 21  22
    j = -- staff.age
    Trace.print("age=" + staff.age + " j=" + j)     // 20  20


    j = staff.salary --
    Trace.print("salary=" + staff.salary + " j=" + j)     // 1999 2000
    var j = -- staff.salary
    Trace.print("salary=" + staff.salary + " j=" + j)     // 1998  1998

    j = (staff.salary /= 2)
    Trace.print("salary=" + staff.salary + " j=" + j)     // 999 999

    j = staff.fav[1] --
    Trace.print("fav[1]=" + staff.fav[1] + " j=" + j)     // 1 2
    var j = -- staff.fav[1]
    Trace.print("fav[1]=" + staff.fav[1] + " j=" + j)     // 0  0
}

fun main(){
    test1()
    test2()
}

