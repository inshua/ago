interface I{
    metaclass{
        final foo as string = $"
            This is some
                text
                    from
                        I.foo
        "$
    }
}
fun main(){
    var a = 1;
    var b = 2;

    var s = $"a + b =  `the result`
                       ${a+b}"$

    Trace.print(s)

    var literal = "\"Hello\nWorld! \u0041\x42\"";
    Trace.print(literal);
    // Prints:
    // Hello
    // World! AB

    Trace.print(I.foo)
}